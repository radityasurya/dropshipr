{
  "extends": "airbnb",
  "plugins": [
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "no-console": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-global-assign": "off"
  },
  "env": {
    "es6": true,
    "mocha": true
  },
  "settings": {
    "react": {
      "version": "999.999.999"
    }
  }
}